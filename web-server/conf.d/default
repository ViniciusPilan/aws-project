server {
    listen       80;
    server_name  localhost;
    root   /var/www/html;

    location / {
        index  index.html;
    }

    error_page   400 /400.html;
    error_page   403 /403.html;
    error_page   404 /404.html;
    error_page   502 /502.html;
    error_page   504 /504.html;
      
}
